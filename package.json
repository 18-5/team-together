{
  "name": "team-together",
  "version": "1.0.0",
  "scripts": {
    "install-root": "npm install",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "install-all": "npm-run-all install-root install-client install-server",
    "watch-client": "cd client && npm run dev",
    "watch-server": "cd server && npm run dev",
    "dev": "npm-run-all --parallel watch-client watch-server",
    "start": "cd server && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "localdb": "sudo /etc/init.d/mysql start && ifconfig",
    "stop": "cd server && npm run stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18-5/team-together.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/18-5/team-together/issues"
  },
  "homepage": "https://github.com/18-5/team-together#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
